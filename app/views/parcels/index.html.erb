<p id="notice"><%= notice %></p>

<h1>Parcels</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Current year value</th>
      <th>Previous year value</th>
      <th>Total taxes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parcels.each do |parcel| %>
      <tr>
        <td><%= parcel.address %></td>
        <td><%= parcel.current_year_value %></td>
        <td><%= parcel.previous_year_value %></td>
        <td><%= parcel.total_taxes %></td>
        <td><%= link_to 'Show', parcel %></td>
        <td><%= link_to 'Edit', edit_parcel_path(parcel) %></td>
        <td><%= link_to 'Destroy', parcel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parcel', new_parcel_path %>

<div class="row">
  <div class="col-md-12">
    <%= will_paginate @parcels %>
  </div>
</div>
